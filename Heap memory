#include<iostream>
using namespace std;
int main(){
   
    int how ;
    cin>>how;
    int *p;
    p=(int*)malloc(how*4);
    cout<<p<<endl;
    
    for (int i= 0 ; i<how;i++){
        cin>>p[i];
    }
    endl(cout);
    endl(cout);
    
    for (int j= 0 ; j<how;j++){
        cout<<p[j]<<endl;
    }
    
     
    int more;
    cout<<"enter for more";
    cin>>more;
    int *q;
    q=(int*)malloc(more*4);
    cout<<q<<endl;
    
    
    for (int m= 0 ; m<how;m++){
       q[m]=p[m];
    }
    
    for (int k= 0 ; k<more;k++){
        cout<<q[k]<<endl;
    }
    
    free(p);
    p=NULL;
    cout<<p<<endl;
    return 0;
}
