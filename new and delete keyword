//NEW AND DELETE -- c++ 

//allocate with help malloc--> free --> free function --> heap memory is limited -->p=NULL  

//int* y = (int*)malloc(2*sizeof(int)) ;
#include <iostream>
using namespace std;
int main(){
    int*h = new int[2];//new = looking for space from heap 
    // 2 and 5 value 
    *h=5;
    *(h+1)=10;
    cout<<h<<endl; 
    cout<<*h<<endl;
    delete []h;//free
    h=NULL; 
    cout<<h<<endl; 
    return 0 ;
}
